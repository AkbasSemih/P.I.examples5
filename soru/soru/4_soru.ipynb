    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv('country_vaccination_stats.csv')\n",
    "\n",
    "df['daily_vaccinations'] = df.groupby('country')['daily_vaccinations'].transform(lambda x: x.fillna(x.min() if x.min() > 0 else 0))\n",
    "\n",
    "df.head(10)"
   ]
